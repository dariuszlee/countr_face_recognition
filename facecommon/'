package countr.common;

import java.io.Serializable;
import java.util.Arrays;

public class RecognitionResult implements Serializable extends ServerResult {
    final float[] result;

    public RecognitionResult(float[] result, boolean success) {
        this.result = result;
        super(success);
    }

    public float[] getResult() {
        return result;
    }

    @Override
    public String toString() {
        return "RecognitionResult [result=" + Arrays.toString(result) + ", success=" + success + "]";
    }
}
